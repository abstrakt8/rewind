          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/desktop-frontend"],
          "options": {
            "jestConfig": "apps/desktop-frontend/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
